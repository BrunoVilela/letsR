    "contents" : "# Function to sum the duplcated species in the matrix\n# Bruno Vilela\n\n.unicas <- function(resu){\n  if(!any(duplicated(colnames(resu)))){\n    return(resu)\n  }else{\n    n <- ncol(resu)  \n    nomes <- colnames(resu)\n    for(i in 1:(n-1)){  \n      nome1 <- nomes[i]\n      for(j in 1:n){\n        nome2 <- nomes[j]    \n        if(nome1==nome2){\n          divid <- which((resu[, i]!=0 & resu[, j]!=0))    \n          resu[,i] <- resu[, i]+resu[, j]\n          resu[divid, i] <- resu[divid, i]/2\n        }\n      }\n    }\n    pos <- which(duplicated(nomes))\n    resu <- resu[, -pos]\n    if(is.vector(resu)){\n      nomes <- names(resu)\n      resu <- matrix(resu, ncol=length(resu))\n      colnames(resu) <- nomes                \n    }\n    return(resu)\n  }\n}\n\n",
